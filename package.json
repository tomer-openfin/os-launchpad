{
  "name": "os-launchpad",
  "version": "2.0.0",
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "npm run clean && webpack",
    "check": "gts check",
    "clean": "gts clean && rimraf build",
    "dev": "webpack-dev-server --hot",
    "docs": "redoc-cli bundle -o ./build/docs/index.html ./docs/api.yml",
    "fix": "gts fix",
    "openfin": "node scripts/openfin.js",
    "serve": "node server.js",
    "start": "node scripts/start.js",
    "storybook": "start-storybook -p 6006",
    "storyshots": "jest -u",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@types/dragula": "2.1.33",
    "@types/enzyme": "3.1.14",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.7",
    "@types/lodash-es": "4.17.1",
    "@types/lodash.throttle": "4.1.4",
    "@types/openfin": "34.0.0",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-actions": "2.3.1",
    "@types/storybook__addon-storyshots": "3.4.3",
    "@types/storybook__react": "3.0.9",
    "@types/styled-components": "4.0.2",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "copy-webpack-plugin": "4.5.4",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "file-loader": "2.0.0",
    "gts": "0.8.0",
    "hadouken-js-adapter": "0.36.1-alpha.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "npm": "6.4.1",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "redoc-cli": "0.6.4",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.4",
    "ts-jest": "23.10.4",
    "tslint-react": "3.6.0",
    "typescript": "3.1.3",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "@giantmachines/redux-openfin": "0.7.3",
    "@storybook/addon-actions": "4.0.0",
    "@storybook/addon-info": "4.0.0",
    "@storybook/addon-knobs": "4.0.0",
    "@storybook/addon-links": "4.0.0",
    "@storybook/addon-storyshots": "4.0.0",
    "@storybook/react": "4.0.0",
    "chalk": "2.4.1",
    "dragula": "3.7.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "execa": "1.0.0",
    "formik": "1.3.1",
    "lodash-es": "4.17.11",
    "openfin-layouts": "0.9.3",
    "openfin-notifications": "0.9.4",
    "popper.js": "1.14.4",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-hot-loader": "4.3.11",
    "react-redux": "5.1.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-actions": "2.6.3",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "storybook-react-router": "1.0.1",
    "styled-components": "4.0.3",
    "tooltip.js": "1.3.0",
    "wait-on": "3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "svg",
      "png"
    ],
    "moduleNameMapper": {
      "\\.(svg|jpg|png)$": "<rootDir>/src/utils/empty-module.ts"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.ts"
  }
}
